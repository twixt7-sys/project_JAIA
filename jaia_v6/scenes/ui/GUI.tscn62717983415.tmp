[sub_resource type="GDScript" id="GDScript_xmm1j"]
script/source = "# SprintTouchButton.gd
extends Control
class_name SprintTouchButton

@export var touch_priority := 200
var _active_ids := {}	# id -> true

func _ready() -> void:
	TouchBus.register(self)

func _exit_tree() -> void:
	TouchBus.unregister(self)

func is_inside_touch(pos: Vector2) -> bool:
	return Rect2(global_position, size).has_point(pos)

func on_touch_down(id: int, _pos: Vector2) -> void:
	_active_ids[id] = true
	_update_state()

func on_touch_move(id: int, pos: Vector2) -> void:
	if _active_ids.has(id) and not is_inside_touch(pos):
		_active_ids.erase(id)
		_update_state()

func on_touch_up(id: int, _pos: Vector2) -> void:
	if _active_ids.has(id):
		_active_ids.erase(id)
		_update_state()

func _update_state() -> void:
	ControlsManager.is_sprinting = _active_ids.size() > 0
"

