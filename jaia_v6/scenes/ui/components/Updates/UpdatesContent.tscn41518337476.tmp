script/source = "extends VBoxContainer

const UPDATE_CONTAINER = preload(\"res://scenes/ui/components/Updates/UpdateContainer.tscn\")

func _ready() -> void:
	var dataFile = Utilities.load_json_file(\"res://resources/data/announcements.json\")
	
	
	add_data_entries(dataFile, UpdateContainerScene)

func add_data_entries(dataFile, entry) -> void:
	for i in dataFile:
			var entry: UpdateContainer = UpdateContainerScene.instantiate()
			entry.set_data(
				dataFile[i][0],
				dataFile[i][1],
				dataFile[i][2]
			)
			add_child(entry)
"

